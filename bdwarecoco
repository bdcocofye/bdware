	if Value == "Default" then
		AntiLock.DesyncVel = Vector3.new(9e9, 9e9, 9e9)
	elseif Value == "Sky" then
		AntiLock.DesyncVel = Vector3.new(15, 9e9, 15)
	elseif Value == "Underground" then
		AntiLock.DesyncVel = Vector3.new(15, -9e9, 15)
	end
end)

